    <div class="page-content">
        <div class="row">
            <div class="col-md-12">
                <ul class="page-breadcrumb breadcrumb">

                    <li>
                        <i class="fa fa-home"></i>
                        <a href="">Home</a>
                        <i class="fa fa-angle-right"></i>
                    </li>
                    <li>
                        <a href="#">Movies</a>
                    </li>

                    <li class="pull-right">
                        <div id="dashboard-report-range" class="dashboard-date-range tooltips" data-placement="top" data-original-title="Change dashboard date range">
                            <i class="fa fa-calendar"></i>
                            <span></span>
                            <i class="fa fa-angle-down"></i>
                        </div>
                    </li>

                </ul>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="table-toolbar">
                    <div class="btn-group">
                        <a href="/admin/movie-add"><button id="sample_editable_1_new" class="btn green">
                                Add Movie &nbsp;&nbsp;<i class="fa fa-plus-square"></i>
                            </button></a>
                    </div>
                    <% if(messages.success){%>
                        <div class="alert alerts alert-success alert-dismissible pull-right" role="alert">
                            <span><%= messages.success%></span>
                            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                    <% }%>

                    <% if(messages.error){%>
                        <div class="alert alerts alert-danger alert-dismissible pull-right" role="alert">
                            <span><%= messages.error%></span>
                            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                    <% }%>



                </div>
                <div class="portlet box purple">
                    <div class="portlet-title">
                        <div class="caption"><i class="fa fa-globe"></i> Movies</div>

                    </div>
                    <div class="portlet-body">

                        <table class="table table-striped table-hover table-bordered" id="sample_2">
                            <thead>
                            <tr>
                                <th style="display: none"></th>
                                <th>Movie</th>
                                <th>Movie Name</th>
                                <th>Released Date</th>
                                <th>Review</th>
                                <th>Action</th>

                            </tr>
                            </thead>
                            <tbody>

                            <% try { if(data.length){

                            for(var i = 0;i < data.length;i++) { %>
                            <tr class="odd gradeX">
                                <td style="display: none"></td>
                                <td><%=data[i].movie_name%></td>
                                <td><%=data[i].review%></td>


                                <td class="textform"><%=data[i].released_date.toLocaleDateString()%></td>
                                <td><img src="../assets/img/<%= data[i].image %>" height="50px" width="60px" alt=""></td>


                                <td><button><a data-toggle="modal" data-target="#operation<%=data[i].id%>"><i class='glyphicon glyphicon-adjust' style='color:black'></i></a></button></td>
                            </tr>

                            <div class="modal fade" id="operation<%=data[i].id%>" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                                <div class="modal-dialog modal-dialog1">
                                    <div class="modal-content">
                                        <div class="modal-body">
                                            <div class="alert alert-danger" style="backgtround-color:red">
                                                <strong>Do Your Choice!!</strong>
                                            </div>
                                            <center>
                                                <button class="modal-button"><a href='/admin/movie-edit/<%=data[i].id%>'><span class='glyphicon glyphicon-edit' style='color:darkred'></span>&nbsp;Edit Movie</a></button>
                                                <br><br>
                                                <button class="modal-button"><a data-toggle="modal" data-target="#myModal1<%=data[i].id%>"><i class='fa fa-trash-o' style='color:blueviolet'></i>&nbsp;Delete Movie</a></button>
                                                <br><br>

                                            </center>

                                        </div>
                                        <div class="modal-footer">
                                            <button type="button" class="btn btn-default" data-dismiss="modal" style="background-color: red">X</button>

                                        </div>
                                    </div>
                                </div>
                            </div>


                            <div class="modal fade" id="myModal1<%=data[i].id%>" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                                <div class="modal-dialog">
                                    <div class="modal-content">
                                        <div class="modal-body">
                                            <div class="alert alert-danger" style="backgtround-color:red">
                                                <strong>Do you want to delete?</strong>
                                            </div>
                                        </div>
                                        <div class="modal-footer">
                                            <button type="button" class="btn btn-default" data-dismiss="modal">No</button>
                                            <a href="/admin/movie-delete/<%=data[i].id%>" class="btn btn-primary">Ok</a>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <%  }}else{ %>
                                <tr>
                                    <td colspan="15">No Data found
                                    </td>
                                </tr>
                            <% } }catch (ex){}%>

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>


</div>

