                <div class="portlet-body form">

                    <%try{%>

                        <form class="form-horizontal form-bordered" action="/admin/movie-save" role="form" method='post' enctype="multipart/form-data">

                            <div class="form-body">


                                <div class="form-group">
                                    <label class="col-md-3 control-label">Movie Image:</label>
                                    <div class="col-md-9">
                                        <input type="file" class="form-control" id="image" name="image"  required >
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="col-md-3 control-label">Movie Name:</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" id="name" name="movie_name"  required >

                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="col-md-3 control-label">Released Date:</label>
                                    <div class="col-md-9">

                                        <input type="date" class="form-control" id="name" name="released_date"  required >
                                    </div>
                                </div>



                                <div class="form-group">
                                    <label class="col-md-3 control-label">Review:</label>
                                    <div class="col-md-9">

                                        <textarea name="review" id="name" class="form-control" required ></textarea>
                                    </div>
                                </div>


                                <div class="form-actions ">
                                    <div class="col-md-offset-3">
                                        <button type="submit" name="submit" class="btn green">Submit</button>
                                        <button type="reset" class="btn default">Cancel</button>
                                    </div>
                                </div>


                            </div>
                        </form>

                    <%}catch (ex){}%>
                </div>
            </div>
        </div>

